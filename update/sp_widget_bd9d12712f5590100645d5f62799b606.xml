		if($('#cars :selected').text() === 'Saab') $('#vehicle2').prop('checked', true);
		if($('#cars :selected').text() === 'Mercedes') $('#vehicle3').prop('checked', true);
	});
	c.mapBeds = function(){
		//spModal.alert('button /is clicked');
		c.server.update().then(function(r){
			spUtil.update($scope);
			if(r.message != ''){
				//alert(r.message);
			}
			c.loadingData = false;
			$window.location.replace(r.link);
			//cabrillo.navigation.goto(r.link, null);
			//$window.location.reload();
		});
	}
}]]></client_script>
